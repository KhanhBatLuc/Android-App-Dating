<?xml version="1.0" encoding="utf-8"?>

<layout xmlns:android="http://schemas.android.com/apk/res/android">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:gravity="center"
        android:orientation="vertical">

        <ImageView
            android:layout_width="345dp"
            android:layout_height="447dp"
            android:layout_marginBottom="@dimen/space16"
            android:contentDescription="@string/stringImageDescription"
            android:src="@drawable/logo" />

        <com.example.dating.customfonts.TextViewRobotoRegular
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginBottom="@dimen/space32"
            android:text="Ứng dụng hẹn hò"
            android:textSize="18sp"
            android:textColor="@color/colorPrimaryDark"/>

        <ProgressBar
            android:layout_marginTop="@dimen/space32"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"/>


    </LinearLayout>

</layout>
